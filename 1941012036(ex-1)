Java:
1 . import java.io.*;
import java.util.*;
public class A1Q1 {
public static void main(String[] args) {
		Scanner sc=new Scanner (System.in);
		int c=0;
		System.out.println("enter length");
		int l=sc.nextInt();
		String a[]=new String [l];
		for(int i=0;i<l;i++)
		{
			System.out.println("enter a string");
			a[i]=sc.next();
		}
		for(int i=0;i<l-1;i++)
		{
			c=0;
			for(int j=0;j<l;j++)
			{
				if(a[i].equals(a[j]))
{
					c=c+1;
				}
			}
			if(c==1)
			{
				System.out.print(a[i]+", ");
			}
		}
	}
}


2 . public class shop extends Market {

	@Override
	void getPrice() {
		// TODO Auto-generated method stub
		System.out.println("price is 100 rupees");
	}
@Override
	void getProductName() {
		// TODO Auto-generated method stub
		System.out.println("Product is deo");
		
	}
	
	Market obj=new shop();
	obj.getPrice();
	obj.getProductName();
}

3 . public class secondLargest {
    static void printsecondLargest(int arr[],int size){
        int i,first,second;
        if (size < 2) {
            System.out.print(" Invalid Input ");
            return;
        }

        first = second = Integer.MIN_VALUE;
        for (i = 0; i < size; i++) {

            if (arr[i] > first) {
                second = first;
                first = arr[i];
            }
else if (arr[i] > second && arr[i] != first)
                second = arr[i];
}
        System.out.print("The second largest element"
                + " is " + second);
    }

    public static void main(String[] args) {
        int arr[] = { 1, 25, 12, 30, 14, 2 };
        int n = arr.length;
        printsecondLargest(arr,n);
    }
}



